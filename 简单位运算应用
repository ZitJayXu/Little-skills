#include<iostream>
using namespace std;
//位运算，一个数组中有两种数出现奇数次和偶数次，找奇数次的数
int findoddnum(int arr[],int n) {
	int Eor = arr[0] ;
	for (int i = 1; i <n ; i++) {
		Eor = Eor ^ arr[i];
	}
	return Eor;
}
int main() {
	int list[11] = { 2,2,2,3,3,3,2,2,2,2,2 };
	cout << findoddnum(list,11) << endl;
	return 0;
	system("pause");
}
